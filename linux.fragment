# enable squashfs
CONFIG_SQUASHFS=y
CONFIG_SQUASHFS_FILE_DIRECT=y
CONFIG_SQUASHFS_DECOMP_MULTI_PERCPU=y
# enable f2fs
CONFIG_F2FS_FS=y
# vintage_net
CONFIG_IP_ADVANCED_ROUTER=y
CONFIG_IP_MULTIPLE_TABLES=y
# enable wireguard
CONFIG_WIREGUARD=m
# enable watchdog
CONFIG_WATCHDOG=y
CONFIG_WATCHDOG_NOWAYOUT
CONFIG_BCM2835_WDT=y
